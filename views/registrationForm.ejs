<!doctype html>
<html lang="en">
<head>
   <% include bootstrap.ejs %>
    <title>Регистрация</title>
    <style>
        body {
            padding:30px;
        }
    </style>
</head>
<body>

<div class="container">
    <!-- Форма Регистрации -->
    <div class="row  align-items-center">
        <form method="post">
            <div class="form-row">
                <div class="form-group col-md-6">
                    <label for="inputEmail4">Email</label>
                    <input type="email" name="email" class="form-control" id="inputEmail4" placeholder="Email">
                </div>
                <div class="form-group col-md-6">
                    <label for="inputPassword4">Пароль</label>
                    <input type="password" name="password" class="form-control" id="inputPassword4"
                           placeholder="Пароль">
                </div>
            </div>
            <div class="form-group">
                <label for="inputAddress">Фамилия</label>
                <input type="text" name="secondname" class="form-control" id="inputAddress"
                       placeholder="Введите фамилию">
            </div>
            <div class="form-group">
                <label for="inputAddress2">Имя</label>
                <input type="text" name="firstname" class="form-control" id="inputAddress2" placeholder="Введите имя">
            </div>
            <button type="submit" class="btn btn-primary">Зарегистрироваться</button>
        </form>
    </div>
</div>
</body>
</html>